<Window x:Class="CoreWpfApp.TestConfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoreWpfApp"
        mc:Ignorable="d"
        Title="TestConfig" Height="450" Width="800"
        WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded">
    <Grid>
        <Button Content="Write" x:Name="btnWrite" HorizontalAlignment="Left" Margin="45,37,0,0" VerticalAlignment="Top" Click="btnWrite_Click"/>
        <Button Content="Read" x:Name="btnRead" HorizontalAlignment="Left" Margin="131,37,0,0" VerticalAlignment="Top" Click="btnRead_Click"/>
        <Button Content="Struct" x:Name="btnStruct" HorizontalAlignment="Left" Margin="231,37,0,0" VerticalAlignment="Top" Click="btnStruct_Click"/>
    </Grid>
</Window>
